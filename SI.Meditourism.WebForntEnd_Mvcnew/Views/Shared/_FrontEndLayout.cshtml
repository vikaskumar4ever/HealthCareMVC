            <label for="country"><b>Country</b><span style="color:red">*</span></label>
            <input type="text" placeholder="Enter Country" name="country" required>


            @if (ViewBag.IsEmailIdExists != null)
            {
                <label style="color:red">@ViewBag.IsEmailIdExists</label>
            }
            <button type="submit" class="button02">Register</button>
        </div>
        <div class="col-6 ab">
            <div style="float:right; margin-right:17%" class="_mYSymnWLg">
                <span style="color:#242424; font-size:13px; font-weight:500">Go Back To</span>
