                CreatedBy: null,
                CreationDate: null,
                UpdatedBy: null,
                UpdatedDate: null,
                IsActive: true,
                IsDeleted: false,
                Id: 0
            };
            $.ajax({
                url: "http://cp.mysamaritanz.com/adminapi/api/GetQuotation",
                type: "POST",
                data: collection,
