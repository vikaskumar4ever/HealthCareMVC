    <script type="text/javascript">

        function GetSearchTreatmeantdetails() {
            console.log($("#txtSearchServices").val())
            if ($("#txtSearchServices").val() != '' && $("#txtSearchServices").val() != undefined) {

                //Load Data function
                var ServiceId;
                $.ajax({
                    // url: "http://localhost:21632/api/GetTreatmentDetailsByName/" + $(".inputsearch").val(),
                    url: "http://cp.mysamaritanz.com/adminapi/api/GetTreatmentDetailsByName/" + $("#txtSearchServices").val(),
                    //url: "http://meditourismadmin.morepower.com/adminapi/api/GetTreatmentDetailsByName/" + $("#txtSearchServices").val(),
                    type: "GET",
                    contentType: "application/json;charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        ServiceId = result.id;
                        loadData(ServiceId);
                    }
                });

                function loadData(Id) {
                    let dropdown1 = $('#selectservices1');
                    dropdown1.empty();

                    $.ajax({
                        //url: "/Home/List/" + Id,
                        url: "http://cp.mysamaritanz.com/adminapi/api/GetTreatMentDetails/" + Id,
                        //url: "http://meditourismadmin.morepower.com/adminapi/api/GetTreatMentDetails/" + Id,
                        //url: "http://localhost:21632/api/GetTreatMentDetails/" + Id,
                        type: "GET",

                        contentType: "application/json;charset=utf-8",

                        dataType: "json",
                        success: function (result) {
                            var i = 0;
                            var html = '';
                            reresult = JSON.parse(result);
                            $.each(reresult, function (key, item) {
                                if (key == "GetService") {
                                    html += '<h1 class="product-item-name" style="text-transform:none;">' + item.Name + '</h1>';
                                    html += '<hr style="margin-bottom:2px; margin-top:-2px; border-top:1px solid black;"/>';
                                    html += '<p style="text-transform:none"p>' + item.Description + '</p>';
                                    dropdown1.append($('<option></option>').attr('value', item.ServiceId).text(item.Name));
                                } else {
                                    if (item.length != 0) {
                                        if (i == 0) {
                                            dropdown1.empty();
                                        }
                                        if (i > 0) {
                                            html += '<h4 class="product-item-name" style="text-transform:none">' + item[i].ServiceName + '</h4>';
                                            html += '<p style="text-transform:none"p>' + item[i].Description + '</p>';
                                        }

                                        dropdown1.append($('<option></option>').attr('value', item[i].ServiceId).text(item[i].ServiceName));
                                        console.log(dropdown1);
                                        i = i + 1;
                                    }
                                }
                            });

                            $('#treatmentdetails').html(html);
                            document.getElementById('_MdgdqtId').style.display = 'block';
                        },

                        error: function (errormessage) {
                            alert(result);
                            alert(errormessage.responseText);

                        }

                    });

                }
                document.getElementById('id055').style.display = 'none';
            }
        }
        function submitMessage() {
            var collection = {
                Name: $("#Name").val(),
                MobileNo: $("#txtPhoneno1").val(),
                EmailId: $("#Email").val(),
                ServiceId: $("#selectservices").val(),
                Country: $("#txtCountry").val(),
